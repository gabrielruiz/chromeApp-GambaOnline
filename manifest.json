{
  "manifest_version": 3,

  "name": "Gamba 106.3 FM Online",
  "short_name": "Gamba FM Online",
  "description": "This app stream Gamba 106.3 FM online",
  "version": "1.4.0",

  "action": {
    "default_icon": {
      "19": "icons/icon.png",
      "38": "icons/icon.png"
    },
    "default_title": "Gamba FM"
  },

  "icons": {
    "19": "icons/icon.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "background": {
    "service_worker": "service_worker.min.js"
  },

  "options_page": "options.html",

  "permissions": [
    "storage",
    "offscreen"
  ],
  "host_permissions": [
    "https://*.hostradios.com:*/*",
    "http://107.150.51.178:10630/*",
    "https://raw.githubusercontent.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
